<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ImageViewSetting">
        <item name="android:layout_width">@dimen/icon_size</item>
        <item name="android:layout_height">@dimen/icon_size</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:tint">@color/YP_white</item>
    </style>

    <style name="SearchFrame">
        <item name="android:backgroundTint">@color/YP_white</item>
        <item name="android:drawableTint">?attr/colorOnPrimary</item>
        <item name="android:drawableTintMode">?attr/colorOnPrimary</item>
        <item name="android:textColorHint">?attr/colorOnPrimary</item>
    </style>

    <style name="SubtitleTrackViewHolder">
        <item name="android:textSize">11sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:layout_marginBottom">14dp</item>
        <item name="android:drawableTint">@color/YP_white</item>
        <item name="android:textColor">@color/YP_white</item>
    </style>

    <style name="ImageViewTrackViewHolder">
        <item name="android:tint">@color/YP_white</item>
    </style>

</resources>